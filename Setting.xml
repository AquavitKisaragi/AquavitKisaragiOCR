<?xml version="1.0" encoding="utf-8"?>
<BmpTalkInfo xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <!-- 重複チェック数 -->
  <Deduplication>5</Deduplication>
  <Language>ja-JP</Language>
  <!-- 読上間隔 -->
  <UpdateTime>1000</UpdateTime>
  <!-- 読取間隔 -->
  <ScanTime>1000</ScanTime>
  <!-- 読取開始位置 -->
  <startPoint>
    <X>0</X>
    <Y>0</Y>
  </startPoint>
  <!-- 読取終了位置 -->
  <endPoint>
    <X>0</X>
    <Y>0</Y>
  </endPoint>
  <!-- 読上必須文字 -->
  <CharacterExtraction></CharacterExtraction>
  <!-- 読み上げアプリケーション設定 -->
  <TalkApp>softalk\SofTalk.exe</TalkApp>
  <TalkAppArguments>/x:1 /w:</TalkAppArguments>
  <EndTalkApp>softalk\SofTalk.exe</EndTalkApp>
  <EndTalkAppArguments>/close</EndTalkAppArguments>
  <KillTalkApp>SofTalk.exe</KillTalkApp>
  <!-- 同一文書チェック率 -->
  <MatchRate>0.95</MatchRate>
  <!-- 最低読上文字数 -->
  <MinLength>2</MinLength>
  <whiteList></whiteList>
  <OutputFileName>AquavitKisaragi.txt</OutputFileName>
</BmpTalkInfo>