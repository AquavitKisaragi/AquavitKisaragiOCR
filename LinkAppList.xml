<LinkAppList>
	<LinkApp Type="ExistFile" ID="0" Info="WAVファイルから再生" >
		<!-- 読み上げアプリケーション設定 -->
		<!-- 情報 -->
		<Detail>ファイルから音声を再生する場合に使う</Detail>
		<!-- 音声再生連携ソフト起動ファイル -->
		<Rec>false</Rec>
		<Talk>true</Talk>
		<LinkAppName>VoiceLink_WAVPlayer.exe</LinkAppName>
		<!-- 後続アプリケーションへの引数 -->
		<Arguments>
			<LinkAppArgs>
				<text>"text":"$dummyText$"</text>
				<otherArgs>"sperker":1</otherArgs>
			</LinkAppArgs>
		</Arguments>
		<AppKillName>VoiceLink_WAVPlayer</AppKillName>
	</LinkApp>
	<LinkApp Type="NotFile" ID="1" Info="AquesTalk10 F1（いわゆるゆっくり）で録音同時再生" >
		<Detail>AquesTalk10 F1（いわゆるゆっくり）で録音と同時再生する（本体のダウンロード及び配置が必要）</Detail>
		<Rec>true</Rec>
		<Talk>false</Talk>
		<LinkAppName>VoiceLink_Aques.exe</LinkAppName>
		<Arguments>
			<LinkAppArgs>
				<text>"text":"$dummyText$"</text>
				<otherArgs>"bas":0,"spd":70,"vol":100,"pit":60,"acc":100,"lmd":50,"fsc":150,"talk":true</otherArgs>
			</LinkAppArgs>
		</Arguments>
		<AppKillName>VoiceLink</AppKillName>
	</LinkApp>
